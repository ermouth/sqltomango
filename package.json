{
  "name": "sqltomango",
  "version": "1.2.0",
  "description": "Converts SQL statements into CouchDB Mango (Cloudant Query) JSON queries",
  "main": "dist/sqltomango.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "build": "./node_modules/browserify/bin/cmd.js -t -g uglifyify index.js --standalone sqltomango | ./node_modules/uglify-js/bin/uglifyjs > browser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibm-cds-labs/sqltomango.git"
  },
  "keywords": [
    "cloudant",
    "couchdb",
    "mango",
    "query",
    "sql"
  ],
  "author": "Glynn Bird <glynn.bird@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ibm-cds-labs/sqltomango/issues"
  },
  "homepage": "https://github.com/ibm-cds-labs/sqltomango#readme",
  "dependencies": {
    "sql-parser": "^0.5.0"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "mocha": "^3.4.2",
    "uglify-js": "^3.0.15",
    "uglifyify": "^3.0.4"
  }
}
